<!-- myloginform  -->
<div class="modal fade" id="myloginForm" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-sm">
        <div class="modal-content" >
            <div class="modal-header" style="background-color:aliceblue;">
                <h4 class="modal-title" style="margin-left:90px;color:lightgray; font-weight: bold">Log in</h4>
            </div>
		    <div class="model-body" style="margin: 15px">
			    <form method="post">
				    <div class="input-group" >
					    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
					    <input type="text" class="form-control" id="loguser" name="loguser" tabindex="1" placeholder="Id" required>
				    </div><br />
				    <div class="input-group">
					    <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
					    <input type="password" class="form-control" id="logpassword" name="logpassword" tabindex="2" placeholder="Password" required>
				    </div>
				    <!-- <div  style="justify-content: flex-end; display:flex">
                        <input type="checkbox" class="ch" id="ckshowpass" name="example">
                        <label class="custom-control-label" for="switch">Hiện mật khẩu</label>
                    </div> -->
				    <div class="form-group" style="margin-top:20px">
					    <button id="loginuser" class="btn  btn-block" style="background-color: orange;font-weight:600;color:#fff;font-size:20px" tabindex="3">Sign In</button>
				    </div>
			    </form>
		    </div>
		    <div class="modal-footer">
			    <a data-toggle="modal"  href="#myForm" id="clearcolor"data-dismiss="modal">Sign Up</a>
			    <button type="button" class="btn"style="background-color: orange;font-weight:600;color:#fff;font-size:16px" id="clearcolor" data-dismiss="modal">Close</button>
		    </div>  
        </div>    
    </div>
</div>
